      {
        "id": "set-segment-brightness.light",
        "title": {
          "en": "Control the brightness of a segment of the light"
        },
        "hint": {
          "en": "Sets the segment to a specific brightness"
        },
        "titleFormatted": {
          "en": "Set the segment [[segmentNr]] or [[segmentArray]] to brightness [[segmentBrightness]]"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=govee-device&capabilities=segmentControlBrightness.light"
          },
          {
            "type": "autocomplete",
            "name": "segmentNr",
            "title": {
              "en": "Segment"
            },
            "required": false,
            "placeholder": {
              "en": "Segment"
            }
          },
          {
            "type": "text",
            "name": "segmentArray",
            "title": {
              "en": "Segment list"
