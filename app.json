    "actions": [
      {
        "id": "activate-music-mode.appliance",
        "title": {
          "en": "Activate music mode"
        },
        "hint": {
          "en": "Activates the music mode in the specified mode and sensitivity"
        },
        "titleFormatted": {
          "en": "Activate music mode [[musicMode]] with sensitivity [[sensitivity]]"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=govee-appliance&capabilities=musicMode.appliance"
          },
          {
            "type": "autocomplete",
            "name": "musicMode",
            "title": {
              "en": "Music mode"
            },
            "placeholder": {
              "en": "Music mode"
            }
          },
          {
            "type": "range",
            "name": "sensitivity",
            "title": {
              "en": "Sensituvity"
            },
            "placeholder": {
              "en": "Sensitivity"
            },
            "labelDecimals": 0,
            "min": 0,
            "max": 100,
            "step": 1,
            "label": "%",
            "labelMultiplier": 1
          }
        ]
      },
      {
        "id": "activate-snapshot.appliance",
        "title": {
          "en": "Activate snapshot"
        },
        "hint": {
          "en": "Activates a previously created snapshot for yor appliance configuration"
        },
        "titleFormatted": {
          "en": "Activate snapshot [[snapshot]]"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=govee-appliance&capabilities=snapshots.appliance"
          },
          {
            "type": "autocomplete",
            "name": "snapshot",
            "title": {
              "en": "Snapshot"
            },
            "placeholder": {
              "en": "game setup"
            }
          }
        ]
      },
      {
        "id": "activate-dreamview.light",
        "title": {
          "en": "Toggle the dream view mode"
        },
        "titleFormatted": {
          "en": "Activate the dreamview mode [[activate]]"
        },
        "titleTrue": {
          "en": "Activate dream view mode"
        },
        "titleFalse": {
          "en": "Deactivate dream view mode"
        },
        "insightsTitleTrue": {
          "en": "Dream view mode was activated"
        },
        "insightsTitleTFalse": {
          "en": "Dream view mode was deactivated"
        },
        "hint": {
          "en": "Activates or deactivates the dream view mode that is supported by the light (music or video)"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=govee-device&capabilities=dreamViewToggle.light"
          },
          {
            "type": "checkbox",
            "name": "activate",
            "title": {
              "en": "Activate"
            },
            "placeholder": {
              "en": "Activate"
            }
          }
        ]
      },
      {
        "id": "activate-music-mode.light",
        "title": {
          "en": "Activate music mode"
        },
        "hint": {
          "en": "Activates the music mode in the specified mode and sensitivity"
        },
        "titleFormatted": {
          "en": "Activate music mode [[musicMode]] with sensitivity [[sensitivity]]"
        },
        "args": [
          {
            "type": "device",
