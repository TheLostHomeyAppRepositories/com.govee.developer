            "required": false,
            "placeholder": {
              "en": "1,3,5,..."
            }
          },
          {
            "type": "range",
            "name": "segmentBrightness",
            "title": {
              "en": "Segment brightness"
            },
            "labelDecimals": 0,
            "min": 0,
            "max": 100,
            "step": 1,
            "label": "%",
            "labelMultiplier": 1,
            "placeholder": {
              "en": "Brightness"
            }
          }
        ]
      },
